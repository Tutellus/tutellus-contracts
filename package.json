{
  "name": "tutellus-contracts",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "standard --verbose | snazzy",
    "lint:js:fix": "standard --verbose --fix | snazzy",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "coverage": "hardhat coverage --solcoverjs ./config.solcover.js",
    "interface": "hardhat gen-interface",
    "concatAbi:v2": "node concatAbi.js --root=artifacts/contracts/v2",
    "concatAbi:v2:minimal": "node concatAbi.js --root=abi/contracts/v2 --minimal=true",
    "concatABI:v2:launchpad": "node concatABI.js --root=contracts/v2:launchpad",
    "claimClients": "npx hardhat run scripts/mainnet/clients/claim.js --network polygon",
    "updateClients": "npx hardhat run scripts/mainnet/clients/update.js --network polygon",
    "resetClients": "npx hardhat run scripts/mainnet/clients/reset.js --network polygon",
    "supertutellianDeployRewards:anvil": "npx hardhat run scripts/testnet/supertutellian/0_deploy_rewardsVault.js --network anvil",
    "supertutellianDeployRewards:goerli": "npx hardhat run scripts/testnet/supertutellian/0_deploy_rewardsVault.js --network goerli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "0.3.1",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.4",
    "@nomiclabs/hardhat-ganache": "^2.0.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/contracts": "^4.2.0",
    "@openzeppelin/contracts-upgradeable": "^4.2.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.35",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "axios": "^1.2.2",
    "csv": "^5.5.3",
    "date-fns": "^2.29.3",
    "dotenv": "^8.2.0",
    "ipfs-http-client": "^50.1.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.6.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "buidler-gas-reporter": "^0.1.4",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.7.2",
    "eventsource": "^2.0.2",
    "hardhat": "^2.10.1",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-interface-generator": "0.0.6",
    "hardhat-preprocessor": "^0.1.4",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.2",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run \"lint\""
    }
  },
  "standard": {
    "ignore": [
      "contracts",
      "build"
    ],
    "globals": [
      "describe",
      "web3",
      "before",
      "beforeEach",
      "it",
      "task",
      "expect",
      "import",
      "ethers",
      "artifacts"
    ]
  }
}
